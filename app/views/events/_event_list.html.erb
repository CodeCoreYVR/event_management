      <table class="table">
        <% @events.each do |event| %>
        <tr class="link" data-id="#<%=event.id%>">
          <td><a href="#<%=event.id%>">
            <p class="center"><%= formatted_date(event.date) %></p>
             <p class="center bold"><%= formatted_time(event.date) %></p></a></td>
          <td><a href="#<%=event.id%>"><%=event.title%></a></td>
        </tr>
        <% end %>

      </table>