
<section id="photo1">
<div class="well">
  <h2  id="titles">We've created a registration tool just for you, made by people just like you.</h2>
  <div class="clear-float"></div>
  <h1 class="center">Please join us for Tech Talks & Free Food.</h1>

  <div class="clear-float"></div>
  <% b = "Are You Ready?"%>
  <% b.split(" ").each do |y| %>
  <% y.split(//).each do |x| %>
  <font class="home-words"><%=x%></font>
  <%end%><font class="home-words">&nbsp</font><%end%>
  <br>&nbsp

  <p class="center">
    <a id="home-button"><%= link_to 'Register', "http://www.codecore.ca", class: "btn btn-warning btn-lg home-button"%></a>
  </p>
  <h6 id="titles">Website is currently under development by CodeCore's 1st Cohort, please visit 
    <a href="http://www.codecore.ca"> codecore's </a> website for updates</h6>
    
    <!-- <h5 id="titles" class="pull-right">Website Design by CodeCore's Cohort #1</h5> -->
    <br>
</section>


<section id="about">
  <h1>About The Event/Speakers</h1>

<div class="row-fluid">
<% @events.each do |event| %>
    <div class="col-sm-6 col-md-4">
      <div class="thumbnail">
        <%= link_to (image_tag event.image(:medium), class: "post-thumb"), event %>
        <div class="caption">
          <%= link_to event.title, post, class: "post-thumb" %>
          <br>
          <p><%= event.body.truncate(100) %></p>
          <p><%= event.date %></p>
          <p><%= event.speaker %></p>
          <p><%= event.address %></p>
          <p><%= event.bio %></p>
          <p><%= event.linkedin %></p>
          <p><%= event.facebook %></p>
          <p><%= event.twitter %></p>
        </div>
      </div>
    </div>
  
<% end %>


  
</section>

